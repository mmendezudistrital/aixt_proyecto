{
  // time module
  "Aixt time sleep": {
    "prefix": "tim",
    "body": "time.${1|sleep,sleep_ms,sleep_us|}(${2:time})",
    "description": "Aixt Time functions"
  },
  // pin module
  "Aixt pin setup": {
    "prefix": "st",
    "body": "pin.setup(pin.${1}, pin.${2|input,output,in_pullup,in_pulldown|})",
    "description": "Aixt Pin setup level function"
  },
  "Aixt pin high": {
    "prefix": "hi",
    "body": "pin.high(pin.${1})",
    "description": "Aixt Pin high level function"
  },
  "Aixt pin low": {
    "prefix": "lo",
    "body": "pin.low(pin.${1})",
    "description": "Aixt Pin low level function"
  },
  // "pin toggle": {
  // 	"prefix": "to",
  // 	"body": "pin.toggle(pin.${1})",
  // 	"description": "Aixt Pin toggle level function"
  // },
  "Aixt pin write": {
    "prefix": "wr",
    "body": "pin.write(pin.${1}, pin.${2:1})",
    "description": "Aixt Pin writing function"
  },
  "Aixt pin read": {
    "prefix": "rd",
    "body": "pin.read(pin.${1})",
    "description": "Aixt pin reading function"
  },
  // port module
  "Aixt port setup": {
    "prefix": "stp",
    "body": "port.setup(port.${1}, ${2:byte})",
    "description": "Aixt 8-bit port setup function"
  },
  "Aixt port read": {
    "prefix": "rdp",
    "body": "port.read(port.${1})",
    "description": "Aixt 8-bit port reading function"
  },
  "Aixt port write": {
    "prefix": "wrp",
    "body": "port.write(port.${1}, ${2:byte})",
    "description": "Aixt 8-bit port writing function"
  },
  // adc module
  "Aixt adc read": {
    "prefix": "rda",
    "body": "adc.read(adc.${1})",
    "description": "Aixt ADC reading function"
  },
  // pwm module
  "Aixt pwm write": {
    "prefix": "wrpw",
    "body": "pwm.write(pwm.${1}, ${2:50})",
    "description": "Aixt PWM writing function"
  },
  // uart module
  "Aixt UART setup": {
    "prefix": "stu",
    "body": "${1|uart|}.setup(${2|300,600,1200,2400,4800,9600,14400,19200,28800,31250,38400,57600,115200|})",
    "description": "Aixt UART character writing function"
  },
  "Aixt UART write": {
    "prefix": "wru",
    "body": "${1|uart|}.write($2)",
    "description": "Aixt UART character writing function"
  },
  "Aixt UART print": {
    "prefix": "pru",
    "body": "${1|uart|}.print($2)",
    "description": "Aixt UART printing string function"
  },
  "Aixt UART println": {
    "prefix": "prlu",
    "body": "${1|uart|}.println($2)",
    "description": "Aixt UART printing string + line new function"
  },
  "Aixt UART read": {
    "prefix": "rdu",
    "body": "${1|uart|}.read()",
    "description": "Aixt UART character reading function"
  },
  "Aixt UART input": {
    "prefix": "inu",
    "body": "${1|uart|}.input(${2:message})",
    "description": "Aixt UART capturing string function"
  }
}
